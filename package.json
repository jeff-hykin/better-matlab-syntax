{
    "name": "better-m-syntax",
    "displayName": "Better MATLAB Syntax",
    "description": "",
    "version": "0.0.3",
    "icon": "icon.png",
    "scripts": {},
    "keywords": [
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "repository": {
        "url": "https://github.com/jeff-hykin/better-matlab-syntax",
        "type": "git"
    },
    "contributes": {
        "languages": [
			{
				"id": "matlab",
				"aliases": [
					"MATLAB",
					"matlab"
				],
				"extensions": [
					".m"
				],
				"configuration": "./language-configuration.json"
			}
		],
        "grammars": [
            {
				"language": "matlab",
				"scopeName": "source.matlab",
				"path": "./autogenerated/m.tmLanguage.json",
                "embeddedLanguages": {
                }
			}
        ]
    },
    "devDependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "textmate-bailout": "^1.1.0",
        "vsce": "^1.99.0",
        "yargs": "^17.5.1",
        "ovsx": "^0.5.1",
        "textmate-tester": "^1.2.0"
    }
}
